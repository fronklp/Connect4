module checkVert(    
    input row,
    input column,
    output reg win
);
if(gameBoard[row][column] == 2'b01 
    && gameBoard[row+1][column] == 2'b01
    && gameBoard[row+2][column] == 2'b01
    && gameBoard[row+3][column] == 2'b01) {
    
    } else if (gameBoard[row][column] == 2'b10 
    && gameBoard[row+1][column] == 2'b10
    && gameBoard[row+2][column] == 2'b10
    && gameBoard[row+3][column] == 2'b10)
endmodule